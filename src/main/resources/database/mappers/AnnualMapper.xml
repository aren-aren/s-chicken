<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.groups.schicken.annual.AnnualDAO">


<select id="annualList" resultType="AnnualVO">
select * from annual where employee_id = #{employeeId}
</select>

<!--  <resultMap id="list" type="EmployeeVO" autoMapping="true">
        <id property="id" column="id"/>
 </resultMap>
 -->

 <select id="list" resultType="EmployeeVO">
  SELECT employee.id, date_of_employment , name FROM employee
</select> 

    
<insert id="annualInsert" parameterType="AnnualVO">
    <selectKey resultType="Long" keyProperty="id" order="BEFORE">
        SELECT nextval(seq5)
    </selectKey>
    insert into annual 
    values 
    (
    #{id},
    #{employeeId},
    #{remainderAnnual},
    0,
    #{history},
    null,
    #{annualDate},
    #{writerId}
    )
    </insert>
    
    
</mapper>
